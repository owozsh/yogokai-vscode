      "settings": {
        "fontStyle": "",
        "foreground": "#d187a1"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type, variable.other.constant.elixir",
      "settings": {
        "fontStyle": "",
        "foreground": "#9ad1bc"
      }
