  "tokenColors": [
    {
      "settings": {
        "foreground": "#c5c9d8"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#c5c9d8"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#505a7f"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#c8e08d"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ce8de0"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#c5c9d8"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#8d9fe0"
      }
    },
    {
      "name": "Property",
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#8d9fe0"
      }
